              do i = 1, A%NRow
                do j = 1, A%NCol
                  if(i.ge.j) then
                    C%matC(i,j) = A%matC(symIndexHash(i,j),1) / B%matC(i,j)
                  else
                    C%matC(i,j) = (-1)*conjg(A%matC(symIndexHash(i,j),1)) / B%matC(i,j)
