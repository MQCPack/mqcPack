!     Check input indicies for consistency
      if(size(D1)*size(D2)*size(D3)*size(D4).ne.1.and. &
        size(D1)*size(D2)*size(D3)*size(D4).ne.2.and. &
        size(D1)*size(D2)*size(D3)*size(D4).ne.4) &
