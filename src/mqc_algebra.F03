        If (MB%Storage.eq.'StorFull') then
          Result = MatMul(Reshape(VA%VecI,[1,MB%NRow]),MB%MatI)
          VC%VecI = Result(1,:)
          !call dgemv('T',MB%nrow,MB%ncol,1.0,MB%mati,MB%nrow,VA%veci,1,0.0,VC%veci,1)
