        elseIf(size(D3).eq.2.or.D3(1).eq.0) then
          IntR4Tensor = MQC_R4Tensor_R4Tensor_At(TensorIn,[D1(1),D1(1)],[D2(1),D2(1)], &
            D3,[D4(1),D4(1)])
