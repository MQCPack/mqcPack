      elseIf(MQC_R4Tensor_HaveComplex(TensorIn)) then
        if(size(D1).eq.2.or.D1(1).eq.0) then
          CompR4Tensor = MQC_R4Tensor_R4Tensor_At(TensorIn,D1,[D2(1),D2(1)], &
            [D3(1),D3(1)],[D4(1),D4(1)])
